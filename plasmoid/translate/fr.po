#: ../contents/config/config.qml
msgid "Entry management"
msgstr "Gestion des entrées"

#: ../contents/config/config.qml
msgid "Debug information"
msgstr "Informations de débogage"

#: ../contents/ui/BootManager.qml
msgid "Firmware Setup"
msgstr "Utilitaire de configuration UEFI"

#: ../contents/ui/BootManager.qml
msgid "Bootloader Menu"
msgstr "Menu du chargeur d'amorçage"

#: ../contents/ui/configEntries.qml
msgid "Toggle and rearrange entries displayed in the main view"
msgstr "Réarrangez et masquez les entrées affichées dans la vue principale"

#: ../contents/ui/configEntries.qml
msgid "Toggle display of this entry"
msgstr "Afficher ou masquer cette entrée"

#: ../contents/ui/configEntries.qml
msgid "Move this entry up the list"
msgstr "Déplacer cette entrée vers le haut"

#: ../contents/ui/configEntries.qml
msgid "Move this entry down the list"
msgstr "Déplacer cette entrée vers le bas"

#: ../contents/ui/configEntries.qml
msgid "No boot entries could be found."
msgstr "Aucune entrée de démarrage n'a pu être trouvée."

#: ../contents/ui/configEntries.qml
msgid "Behavior upon selecting an entry :"
msgstr "Comportement après sélection :"

#: ../contents/ui/configEntries.qml
msgid "Reboot immediately"
msgstr "Redémarrer immédiatement"

#: ../contents/ui/configEntries.qml
msgid "Reboot after confirmation"
msgstr "Redémarrer après confirmation"

#: ../contents/ui/configEntries.qml
msgid "Don't reboot"
msgstr "Ne pas redémarrer"

#: ../contents/ui/configInfo.qml
msgid "systemd is at version 251 or above"
msgstr "systemd est au moins version 251"

#: ../contents/ui/configInfo.qml
msgid "bootctl is present"
msgstr "bootctl est présent"

#: ../contents/ui/configInfo.qml
msgid "Can reboot to the Firmware Setup"
msgstr "Peut redémarrer vers la configuration EFI"

#: ../contents/ui/configInfo.qml
msgid "Can reboot to the Bootloader Menu"
msgstr "Peut redémarrer vers le menu d'armorçage"

#: ../contents/ui/configInfo.qml
msgid "Can reboot to a custom entry"
msgstr "Peut redémarrer vers n'importe quelle entrée"

#: ../contents/ui/configInfo.qml
msgid "Could get the custom entries"
msgstr "A pu récupérer les entrées de démarrage"

#: ../contents/ui/configInfo.qml
msgid "In case of issues or missing entries, please ensure that the requirements shown below are met"
msgstr "En cas de problème, merci de vérifier que les conditions ci-dessous soient bien remplies"

#: ../contents/ui/configInfo.qml
msgid "Reset configuration"
msgstr "Réinitialiser la configuration"

#: ../contents/ui/configInfo.qml
msgid "View log"
msgstr "Voir le journal"

#: ../contents/ui/configInfo.qml
msgid "Reset settings?"
msgstr "Réinitialiser l'applet ?"

#: ../contents/ui/configInfo.qml
msgid "This will reset this plasmoid's configuration and state."
msgstr "Ceci réinitialisera l'applet à son état initial."

#: ../contents/ui/configInfo.qml
msgid "This plasmoid's configuration and state have been reset."
msgstr "La configuration de cet applet a bien été réinitialisée."

#: ../contents/ui/configInfo.qml
msgid "Log"
msgstr "Journal"

#: ../contents/ui/FullRepresentation.qml
msgid "Reboot to..."
msgstr "Redémarrer vers..."

#: ../contents/ui/FullRepresentation.qml
msgid "Reload this applet"
msgstr "Recharger cet applet"

#: ../contents/ui/FullRepresentation.qml
msgid "Configure this applet"
msgstr "Configurer cet applet"

#: ../contents/ui/FullRepresentation.qml
msgid ""
"No boot entries could be listed.\n"
"Please check this applet settings."
msgstr ""
"Aucune entrée de démarrage ne peut être affichée.\n"
"Merci de vérifier les paramètres de cet applet."

#: ../contents/ui/FullRepresentation.qml
msgid "Configure"
msgstr "Configurer"

#: ../contents/ui/FullRepresentation.qml
msgid ""
"This applet cannot work on this system.\n"
"Please check that the system is booted in UEFI mode and that systemd, systemd-boot are used and configured properly."
msgstr ""
"Cet applet ne peut pas fonctionner sur ce système.\n"
"Merci de vérifier que le système est bien configuré pour utiliser UEFI, systemd et bootctl."

#: ../contents/ui/FullRepresentation.qml
msgid "Root access is required to get the full boot entries list."
msgstr "Un accès superutilisateur est nécessaire pour récuperer la liste complète des entrées de démarrage."

#: ../contents/ui/FullRepresentation.qml
msgid "Retry as root"
msgstr "S'authentifier"

#: ../contents/ui/FullRepresentation.qml
msgid "Ignore"
msgstr "Ignorer"

#: ../contents/ui/FullRepresentation.qml
msgid "Advanced reboot"
msgstr "Redémarrage avancé"

#: ../contents/ui/FullRepresentation.qml
msgid "The entry <b>"
msgstr "L'éntrée <b>"

#: ../contents/ui/FullRepresentation.qml
msgid "</b> has been set for the next reboot."
msgstr "</b> a bien été définie pour le prochain démarrage"

